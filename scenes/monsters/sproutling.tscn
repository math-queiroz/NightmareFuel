func depart():
	$AnimationPlayer.play(\"fade\")

func say():
	push_warning(\"Not implemented!\")
		
func on_correct_deliver():
	print_debug(\"Sproutling is happy\")
	depart()

func on_wrong_deliver():
	print_debug(\"Sproutling hates you\")
	depart()

func _on_timeout():
	depart()
